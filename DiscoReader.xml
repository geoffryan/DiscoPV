<ServerManagerConfiguration>
    <ProxyGroup name="sources">
        <!-- ============================================================== -->
        <SourceProxy name="DiscoReader" class="vtkDiscoReader"  
            label="Disco Reader">
            <Documentation
                long_help="Read a Disco Checkpoint (.h5) file."
                short_help="Read a Disco Checkpoint (.h5) file.">
            </Documentation>
            <StringVectorProperty
                name="Filename"
                animateable="0"
                command="SetFileName"
                number_of_elements="1">
                <FileListDomain name="files" />
                <Documentation>
                    This property specifies the file name for Disco Reader.
                </Documentation>
            </StringVectorProperty>
            <IntVectorProperty
                name="Mesh Type"
                command="SetMeshType"
                number_of_elements="1"
                default_values="0">
                <EnumerationDomain name="enum">
                    <Entry value="0" text="Tetra"/>
                    <Entry value="1" text="Polyhedron"/>
                    <Entry value="2" text="Hexahedron"/>
                </EnumerationDomain>
                <Documentation>
                    This property specifies the mesh type to use.
                </Documentation>
            </IntVectorProperty>

            <Hints>
                <ReaderFactory extensions="h5"
                                file_description="Disco Checkpoints" />
            </Hints>
        </SourceProxy>
        <!-- End DiscoReader -->
    </ProxyGroup>
    <!-- End Filters Group -->
</ServerManagerConfiguration>
